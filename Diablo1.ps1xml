							<ScriptBlock>$psitem.ToString()</ScriptBlock>
						</TableColumnItem>
						<TableColumnItem>
							<PropertyName>Quality</PropertyName>
						</TableColumnItem>
					</TableColumnItems>
				</TableRowEntry>
