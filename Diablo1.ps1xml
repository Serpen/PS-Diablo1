					<TableColumnItems>
						<TableColumnItem>
							<PropertyName>Spell</PropertyName>
						</TableColumnItem>
						<TableColumnItem>
							<PropertyName>Spellbook</PropertyName>
						</TableColumnItem>
						<TableColumnItem>
							<PropertyName>Enabled</PropertyName>
						</TableColumnItem>
						<TableColumnItem>
							<PropertyName>Level</PropertyName>
						</TableColumnItem>
